{
  "name": "schaltwerk",
  "version": "0.4.2",
  "type": "module",
  "license": "MIT",
  "author": "Schaltwerk Contributors",
  "description": "AI-powered git worktree session manager for isolated development environments",
  "packageManager": "bun@1.2.16",
  "repository": {
    "type": "git",
    "url": "https://github.com/schaltwerk/schaltwerk.git"
  },
  "bugs": {
    "url": "https://github.com/schaltwerk/schaltwerk/issues"
  },
  "homepage": "https://github.com/schaltwerk/schaltwerk#readme",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit --skipLibCheck && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "SCHALTWERK_START_DIR=$HOME tauri dev",
    "lint": "eslint src scripts --ext .ts,.tsx,.js",
    "lint:ts": "tsc --noEmit",
    "lint:rust": "cd src-tauri && cargo clippy --all-features --message-format=short -- -D warnings",
    "lint:mcp": "cd mcp-server && bun run lint",
    "test:frontend": "ROLLUP_SKIP_NODEJS_NATIVE_ADDON=1 FORCE_COLOR=0 vitest run --silent --reporter=dot",
    "test:frontend:coverage": "ROLLUP_SKIP_NODEJS_NATIVE_ADDON=1 vitest run --coverage",
    "test:frontend:watch": "ROLLUP_SKIP_NODEJS_NATIVE_ADDON=1 vitest",
    "deps:rust": "cd src-tauri && cargo shear",
    "test:rust": "cd src-tauri && CARGO_TERM_COLOR=never RUST_LOG=warn cargo nextest run --all-features --status-level fail --failure-output immediate",
    "test:mcp": "cd mcp-server && bun test",
    "test": "run-s lint lint:ts lint:mcp lint:rust deps:rust test:frontend test:mcp test:rust build:rust",
    "test:fast": "run-p lint lint:ts test:frontend && run-s lint:rust test:rust build:rust",
    "coverage:report": "node scripts/coverage-report.js",
    "coverage:rust:run": "mkdir -p coverage && (cd src-tauri && rustup component add llvm-tools-preview && cargo install cargo-llvm-cov --locked -q) && (cd src-tauri && cargo llvm-cov --lcov --output-path ../coverage/rust-lcov.info)",
    "coverage:rust:report": "node scripts/coverage-report-rust.js",
    "coverage:rust": "run-s coverage:rust:run coverage:rust:report",
    "coverage:all": "run-s test:frontend:coverage coverage:report coverage:rust",
    "coverage": "run-s test:frontend:coverage coverage:report",
    "build:rust": "cd src-tauri && cargo build --message-format=short",
    "check": "run-s test",
    "migrate-events": "node scripts/migrate-events.cjs",
    "validate-migration": "node scripts/validate-migration.cjs"
  },
  "overrides": {},
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.50.1",
    "lightningcss-linux-x64-gnu": "^1.30.1"
  },
  "dependencies": {
    "@anthropic-ai/sandbox-runtime": "^0.0.1",
    "@codemirror/autocomplete": "^6.19.0",
    "@codemirror/lang-markdown": "^6.4.0",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.38.5",
    "@tailwindcss/postcss": "^4.1.14",
    "@tauri-apps/api": "^2.9.0",
    "@tauri-apps/plugin-dialog": "^2.4.0",
    "@tauri-apps/plugin-os": "^2.0.0",
    "@tauri-apps/plugin-updater": "^2.3.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@uiw/react-codemirror": "^4.25.2",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-image": "^0.8.0",
    "@xterm/addon-ligatures": "^0.9.0",
    "@xterm/addon-search": "^0.15.0",
    "@xterm/addon-serialize": "^0.13.0",
    "@xterm/addon-unicode11": "^0.8.0",
    "@xterm/addon-webgl": "^0.18.0",
    "@xterm/xterm": "^5.5.0",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "gpt-tokenizer": "^3.2.0",
    "highlight.js": "^11.11.1",
    "minimatch": "^10.0.3",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-split": "^2.0.14",
    "react-window": "^2.2.1",
    "remark-gfm": "^4.0.1",
    "tailwindcss": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@tauri-apps/cli": "^2.9.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/minimatch": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitest/coverage-v8": "^4.0.1",
    "@vitest/ui": "^4.0.1",
    "eslint": "^9.37.0",
    "eslint-plugin-react-hooks": "^7.0.0",
    "happy-dom": "^20.0.2",
    "npm-run-all2": "^8.0.0",
    "terser": "^5.44.0",
    "typescript": "~5.9.3",
    "vite": "^7.1.9",
    "vitest": "^4.0.1"
  }
}
