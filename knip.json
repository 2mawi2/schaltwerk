{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/main.tsx",
    "src/**/*.test.ts",
    "src/**/*.test.tsx"
  ],
  "project": [
    "src/**/*.{ts,tsx}",
    "!src/**/*.test.{ts,tsx}"
  ],
  "ignore": [
    "src/types/**/*.d.ts",
    "src/test-utils/**",
    "src/test/**"
  ],
  "ignoreDependencies": [
    "@tauri-apps/cli",
    "@testing-library/dom",
    "@vitest/ui",
    "eslint",
    "npm-run-all2",
    "terser"
  ],
  "ignoreBinaries": [
    "vite",
    "tsc",
    "tauri",
    "eslint",
    "vitest",
    "knip",
    "run-s",
    "run-p"
  ],
  "ignoreExportsUsedInFile": true
}
