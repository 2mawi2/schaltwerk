[codespell]
# Ref: https://github.com/codespell-project/codespell#using-a-config-file
skip = .git*,node_modules,target,dist,*.lock,*.log,build,out,.codespellrc,package-lock.json
check-hidden = true
ignore-regex = ^\s*"image/\S+": ".*|\b(afterAll|devlop|vEw|UpToDate)\b
ignore-words-list = tauri,schaltwerk,worktree,worktrees,nextest,clippy,rustfmt,rustc,caf,te,fo,statics,trough
ignore-words = .codespellignore
